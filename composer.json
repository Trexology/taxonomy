{
  "name": "trexology/taxonomy",
  "description": "Create and manage a heirarchical taxonomy of terms within different vocabularies",
  "license": "MIT",
  "keywords": ["taxonomy","laravel"],
  "authors": [
    {
      "name": "Trex Lim",
      "email": "trexlim@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~6.0"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Trexology\\Taxonomy\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Trexology\\Taxonomy\\TaxonomyServiceProvider"
      ],
      "aliases": {
        "Taxonomy": "Trexology\\Taxonomy\\Facades\\TaxonomyFacade"
      }
    }
  }
}
